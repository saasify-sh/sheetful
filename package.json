  "devDependencies": {
    "@types/koa": "^2.11.2",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-compress": "^2.0.9",
    "@types/koa-router": "^7.4.0",
    "@types/node": "^13.7.4",
    "cross-env": "^7.0.0",
    "del-cli": "^3.0.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.2"
